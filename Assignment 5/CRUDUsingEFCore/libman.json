{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/js/",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    },
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/css/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap.css",
        "css/bootstrap.min.css",
        "js/bootstrap.js"
      ]
    },
    {
      "library": "validate.js@0.13.1",
      "destination": "wwwroot/js/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/js/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "jquery-toast-plugin@1.3.2",
      "destination": "wwwroot/jquery-toast-plugin/",
      "files": [
        "jquery.toast.min.js",
        "jquery.toast.min.css"
      ]
    }
  ]
}